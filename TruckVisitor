package com.visitorpattern;

public class TruckVisitor implements Visitor {
   
    public void visit(TruckVariable truckVariable) {
        if (truckVariable.towPackage) {
            System.out.println("We need to buy a boat");
        }
    }

	@Override
	public void visit(CarVariable carVariable) {
		// TODO Auto-generated method stub
		
	}
}
