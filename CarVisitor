package com.visitorpattern;

public class CarVisitor implements Visitor {
    @Override
    public void visit(CarVariable carVariable) {
        if (carVariable.seats > 2) {
            System.out.println("This is clearly a car for old people");
        } else {
            System.out.println("My bet is this car has at least 2 cylinders");
        }
    }
}
